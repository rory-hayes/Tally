# Supabase environment variables used by Next.js (loaded at build time).
# Copy to `.env.local` and fill in actual values.

NEXT_PUBLIC_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
# Prefer the publishable key (starts with sb_publishable_...)
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=YOUR_PUBLISHABLE_KEY
# Optional legacy key fallback if publishable keys are disabled
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_LEGACY_ANON_KEY

# Server-side Supabase configuration (used in API routes)
SUPABASE_SERVICE_ROLE_KEY=YOUR_SERVICE_ROLE_KEY
SUPABASE_ARTEFACTS_BUCKET=artefacts

